<!DOCTYPE HTML>
<html>

<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title>E-Products Web Site</title>
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<meta name="description" content="" />
	<meta name="keywords" content="" />
	<meta name="author" content="" />

	<link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600" rel="stylesheet">
	<link href="https://fonts.googleapis.com/css?family=Nunito:200,300,400" rel="stylesheet">

	<!-- Animate.css -->
	<link rel="stylesheet" href="~/Content/css/animate.css">
	<!-- Icomoon Icon Fonts-->
	<link rel="stylesheet" href="~/Content/css/icomoon.css">
	<!-- Bootstrap  -->
	<link rel="stylesheet" href="~/Content/css/bootstrap.css">

	<!-- Magnific Popup -->
	<link rel="stylesheet" href="~/Content/css/magnific-popup.css">

	<!-- Owl Carousel -->
	<link rel="stylesheet" href="~/Content/css/owl.carousel.min.css">
	<link rel="stylesheet" href="~/Content/css/owl.theme.default.min.css">

	<!-- Theme style  -->
	<link rel="stylesheet" href="~/Content/css/style.css">

	<!-- Modernizr JS -->
	<script src="~/Scripts/js/modernizr-2.6.2.min.js"></script>
	<!-- FOR IE9 below -->
	<!--[if lt IE 9]>
	<script src="~/Scripts/js/respond.min.js"></script>
	<![endif]-->

</head>

<body>

	<div class="colorlib-loader"></div>

	<div id="page">
		<nav class="colorlib-nav" role="navigation">
			<div class="top-menu">
				<div class="container">
					<div class="row">
						<div class="col-md-2">
							<div id="colorlib-logo"><a href="/product/home">E-Products</a></div>
						</div>
						<div class="col-md-10 text-right menu-1">
							<ul>
								<li class="active"><a href="/product/home">Home</a></li>
								<li class="active"><a href="/product/DisplayAll">Products</a></li>
								<li class="active"><a href="/supplier/DisplayAll">Suppliers</a></li>
							</ul>
						</div>
					</div>
				</div>
			</div>
		</nav>

		<section id="home" class="video-hero"
				 style="height: 100px; background-image: url(~/Content/images/cover_img_1.jpg);  background-size:cover; background-position: center center;background-attachment:fixed;"
				 data-section="home">
			<div class="overlay"></div>
			<a class="player"
			   data-property="{videoURL:'https://www.youtube.com/watch?v=vqqt5p0q-eU',containment:'#home', showControls:false, autoPlay:true, loop:true, mute:true, startAt:0, opacity:1, quality:'default'}"></a>
			<div class="display-t text-center">
			</div>
		</section>

		@RenderBody()

		<footer id="colorlib-footer">
			<div class="container">
				<div class="row row-pb-md">

					<div class="col-md-3 colorlib-widget">
						<h4>Information</h4>
						<p>
							<ul class="colorlib-footer-links">
								<li><a href="#"><i></i> Home</a></li>
								<li><a href="/product/DisplayAll"><i></i> Products</a></li>
								<li><a href="/Supplier/DisplayAll"><i></i> Suppliers</a></li>
							</ul>
						</p>
					</div>

					<div class="col-md-5">

					</div>

					<div class="col-md-4 colorlib-widget">
						<h4>About E-Products</h4>
						<p>
							E-Products have a good high quality tech products .
							<br />
							we aim to deliver high-tech products for all customers .
						</p>

					</div>

				</div>
			</div>

		</footer>
	</div>

	<!-- <div class="gototop js-top">
		<a href="#" class="js-gotop"><i class="icon-arrow-up2"></i></a>
	</div> -->
	<!-- jQuery -->
	<script src="~/Scripts/js/jquery.min.js"></script>
	<!-- jQuery Easing -->
	<script src="~/Scripts/js/jquery.easing.1.3.js"></script>
	<!-- Bootstrap -->
	<script src="~/Scripts/js/bootstrap.min.js"></script>
	<!-- Waypoints -->
	<script src="~/Scripts/js/jquery.waypoints.min.js"></script>
	<!-- Stellar Parallax -->
	<script src="~/Scripts/js/jquery.stellar.min.js"></script>
	<!-- YTPlayer -->
	<script src="~/Scripts/js/jquery.mb.YTPlayer.min.js"></script>
	<!-- Owl carousel -->
	<script src="~/Scripts/js/owl.carousel.min.js"></script>
	<!-- Magnific Popup -->
	<script src="~/Scripts/js/jquery.magnific-popup.min.js"></script>
	<script src="~/Scripts/js/magnific-popup-options.js"></script>
	<!-- Counters -->
	<script src="~/Scripts/js/jquery.countTo.js"></script>
	<!-- Main -->
	<script src="~/Scripts/js/main.js"></script>
	@Scripts.Render("~/bundles/jquery")
	@Scripts.Render("~/bundles/bootstrap")
	@RenderSection("scripts", required: false)
</body>

</html>